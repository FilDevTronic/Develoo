<h2 class="text-white">Profiles Directory</h2>
<div class="jumbotron">
  <ul>
    <% @profile.each do |profile| %>
      <li>
        <%= link_to profile.user.alias, profile %>
        <% if current_user == profile.user %>
          <span class="ml-2"><%= link_to 'Edit', edit_profile_path(profile), class: "btn btn-small btn-outline-primary" %></span>
        <% end %>
      </li>
    <% end %>
  </ul>
</div>
